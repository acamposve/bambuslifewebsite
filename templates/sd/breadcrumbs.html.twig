<script type="application/ld+json">{
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
  {% for item in items %}
    {
      "@type": "ListItem",
      "position": {{loop.index}},
      "name": "{{item.name}}",
      "item": "https://bambus.life{{item.url}}"
    }{% if not loop.last %},{% endif %}
  {% endfor %}
  ]
}</script>